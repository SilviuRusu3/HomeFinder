@model HomeFeatures
@{
    ViewData["Title"] = "Feature Details";
}
<h3>@ViewData["Title"]</h3>
<div>
    Features Name: @Model.Name
</div>
<div>
    Home type: @Model.HomeType
</div>
<div class="card-footer text-center">
    <a asp-controller="Features" asp-action="Index" class="btn btn-primary">Back to list</a>
    <a asp-controller="Attributes"  asp-action="Update" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
    <a asp-controller="Attributes" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-primary text-danger">Delete</a>
</div>